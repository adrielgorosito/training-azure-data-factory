					"name": "DimProductos"
				},
				{
					"dataset": {
						"referenceName": "DIM_Fecha",
						"type": "DatasetReference"
					},
					"name": "DimFecha"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DIM_Productos",
						"type": "DatasetReference"
					},
					"name": "InsertDimProducto"
				},
				{
					"dataset": {
						"referenceName": "DIM_Fecha",
						"type": "DatasetReference"
